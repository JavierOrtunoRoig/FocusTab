{
  "manifest_version": 3,
  "name": "FocusTab",
  "version": "0.0.1",
  "description": "Transform your new tab into a productivity hub with customizable widgets.",
  "icons": {
    "16": "icons/icon-16.webp",
    "48": "icons/icon-48.webp",
    "128": "icons/icon-128.webp"
  },
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": [],
  "action": {
    "default_icon": "icons/icon-48.webp",
    "default_title": "FocusTab Settings"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*", "icons/*"],
      "matches": ["*://*/*"]
    }
  ]
}
